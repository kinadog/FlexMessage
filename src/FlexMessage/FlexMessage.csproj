<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>wwwroot\favicon.ico</ApplicationIcon>
    <Title>FlexMessage</Title>
    <Authors>Kinadog</Authors>
    <Description>Easy and versatile client message library for .net web applications</Description>
    <Copyright>Copyright (c) Kinadog 2022</Copyright>
    <PackageProjectUrl>https://github.com/kinadog/FlexMessage</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <PackageReadmeFile>README.en.md</PackageReadmeFile>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/kinadog/FlexMessage</RepositoryUrl>
    <PackageReleaseNotes>https://github.com/kinadog/FlexMessage/releases</PackageReleaseNotes>
    <PackAsTool>False</PackAsTool>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
	  <IsPackable>true</IsPackable>
	  <IsPublishable>False</IsPublishable>
	  <OutputType>Library</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\..\README.en.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
